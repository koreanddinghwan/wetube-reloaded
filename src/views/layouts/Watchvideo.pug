extends default.pug 


block content 
    #videos 
        h1=video.title
        video(src="/" + video.fileUrl)
        div
            button#play Play
            button#mute Mute
            
            input(type="range", step="0.1",min="0", max="1")#volumn
            div
                span#currentTime 00:00
                span /
                span#totalTime 00:00

        div
            input(type="range",step="1", value="0", min="0")#timeline


block scripts
    script(src="/static/js/videoPlayer.js")